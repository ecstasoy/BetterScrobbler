  "title_cleanup_patterns": [
    {
      "pattern": "\\s*[\\(\\[](?:MV|M/V|Music Video|Official Video|Lyric Video|Audio|Visualizer|Karaoke)[^\\)\\]]*[\\)\\]].*$",
      "description": "Music video related suffixes"
    },
    {
      "pattern": "\\s*[\\(\\[](?:Official|HD|4K|Remaster(?:ed)?|[12]\\d{3})[^\\)\\]]*[\\)\\]].*$",
      "description": "Quality and year related suffixes"
    },
    {
      "pattern": "\\s*[\\(\\[](?:Live|Concert|Tour|Session|Performance)[^\\)\\]]*[\\)\\]].*$",
      "description": "Live performance related suffixes"
    },
    {
      "pattern": "\\s*[\\(\\[](?:TV|Show|Episode|Late Night|Television)[^\\)\\]]*[\\)\\]].*$",
      "description": "TV show related suffixes"
    },
    {
      "pattern": "\\s*【[^】]*】.*$",
      "description": "Chinese style brackets and content"
    },
    {
      "pattern": "\\s*\\|\\s*.*$",
      "description": "Vertical bar separator and trailing content"
    },
    {
      "pattern": "\\s*｜\\s*.*$",
      "description": "Fullwidth vertical bar separator and trailing content"
    },
    {
      "pattern": "\\s*\\d{1,2}[-/]\\d{1,2}[-/]\\d{2,4}.*$",
      "description": "Date format (without brackets)"
    },
    {
      "pattern": "\\s*[\\(\\[]\\d{1,2}[-/]\\d{1,2}[-/]\\d{2,4}[\\)\\]].*$",
      "description": "Date format (with brackets)"
    }
  ],
  "title_parse_patterns": [
    {
      "pattern": "(.+?)[-–−﹣－]\\s*['\"](.+?)['\"]",
      "description": "Artist - 'Title' or Artist - \"Title\" format (with quotes)",
      "priority": 1
    },
    {
      "pattern": "(.+?)(?:\\s*[-–−﹣－]\\s*)(.+)",
      "description": "Standard Artist - Title format",
      "priority": 2
    },
    {
      "pattern": "(.+?)[「『](.+?)[」』]",
      "description": "Japanese/Chinese style brackets format",
      "priority": 2
    },
    {
      "pattern": "(.+?)[\\[|\\(](.+?)[\\]|\\)]",
      "description": "Artist [Title] or Artist (Title) format",
      "priority": 2
    },
    {
      "pattern": "(.+?)[-–−﹣－](.+?)\\s*\\(\\d{4}\\)",
      "description": "Artist-Title (Year) format",
      "priority": 2
    },
    {
      "pattern": "(.+?)[-–−﹣－](.+?)\\s*\\[.*?\\]",
      "description": "Artist-Title [...] format",
      "priority": 2
    },
    {
      "pattern": "(.+?)『(.+?)』\\s*\\(\\d{4}\\)",
      "description": "Artist『Title』(Year) format",
      "priority": 2
    },
    {
      "pattern": "(.+?)[-–−﹣－](.+?)\\s*@.*",
      "description": "Artist-Title @ ... format",
      "priority": 2
    },
    {
      "pattern": "(.+?)[-–−﹣－](.+?)\\s*\\((?:live|LIVE)[^)]*\\)",
      "description": "Artist-Title (Live ...) format",
      "priority": 2
    },
    {
      "pattern": "(.+?)[<](.+?)[>]",
      "description": "Artist<Title> format",
      "priority": 2
    }
  ]
}